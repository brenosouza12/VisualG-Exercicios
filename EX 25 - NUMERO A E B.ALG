Algoritmo "EX 5E - NUMERO A E B"

Var
   NumeroA:inteiro
   NumeroB:inteiro


Inicio
   escreva("Digite o primeiro número: ")
   leia(NumeroA)
   escreva("Digite o segundo número: ")
   leia(NumeroB)
   
   se(NumeroA>NumeroB) entao
   escreva("O número A é maior que o número B")
   fimse
   se(NumeroB>NumeroA)entao
   escreva("O número B é maior que o número A")
   fimse


Fimalgoritmo